{
  "version": 2,
  "builds": [
    {
      "src": "main/templates/main/home.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main/templates/main/home.html"
    }
  ]
}
